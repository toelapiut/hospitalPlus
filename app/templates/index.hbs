<div class="container ">
  <div class='addpatient'>
    <img src="image/favicon.ico"><span class="patientbtn">Patient</span>
  </div>
</div>

<div class="container" id="main">
  <table class="table table-striped">
    <thead>
    <tr>
      <th>Patients ID</th>

      <th>First Name</th>

      <th>Last Name</th>

      <th>Gender</th>

      <th>D.O.B</th>

      <th>phone No.</th>

      <th>Emergency No.</th>

      <th>Check Out Time.</th>

      <th>Check In Time.</th>

      <th>Status</th>
    </tr>
  </thead>
    <tbody>
    {{#each model as |patientInfo|}}


<tr>
  <td>{{patientInfo.patientsId}}</td>

  <td>{{patientInfo.firstName}}</td>

  <td>{{patientInfo.lastName}}</td>

  <td>{{patientInfo.sex}}</td>

  <td>{{patientInfo.dob}}</td>

  <td>{{patientInfo.phoneNo}}</td>

  <td>{{patientInfo.emergencyNo}}</td>

  <td>{{patientInfo.checkOuttime}}</td>

  <td>{{patientInfo.checkIntime}}</td>

  <td> {{patientInfo.status}}</td>
</tr>
    {{/each}}

    </tbody>
  </table>
</div>
