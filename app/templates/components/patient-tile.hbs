<h1>Patient Info</h1>
<div class="row ">

  <div class="col-md-2">
    <h5>Patients ID</h5> {{patientdisplay.patientsId}}
  </div>

  <div class="col-md-1">
    <h5>First Name</h5> {{patientdisplay.firstName}}
  </div>

<div class="col-md-2">
    <h5>Last Name</h5> {{patientdisplay.lastName}}
  </div>

  <div class="col-md-1">
    <h5>Gender</h5> {{patientdisplay.sex}}
  </div>

  <div class="col-md-1">
    <h5>D.O.B</h5> {{patientdisplay.dob}}
  </div>

  <div class="col-md-1">
    <h5>phone No.</h5> {{patientdisplay.phoneNo}}
  </div>

  <div class="col-md-1">
    <h5>Emergency No.</h5> {{patientdisplay.emergencyNo}}
  </div>

  <div class="col-md-1">
    <h5>Status</h5>

    {{#if checkInclick}}
      <button class="btn btn-success" {{action 'checkIn'}}> CHECK IN </button>
       {{else}}
      <button class="btn btn-danger" {{action 'checkOut'}}> CHECK OUT </button>
    {{/if}}
  </div>
</div>
